<mvc:View controllerName="app.app.controller.App" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" displayBlock="true">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="Drone Management">
					<content>
						<VBox class="sapUiMediumMargin">
							<Button text="Import Parts" press="onNavigateToImportParts" class="sapUiTinyMarginBottom" />
							<Button text="Export Drones" press="onNavigateToExportDrones" />
						</VBox>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>